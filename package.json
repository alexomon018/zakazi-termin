{
  "name": "zakazi-termin",
  "version": "0.1.0",
  "private": true,
  "packageManager": "yarn@4.12.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "db:generate": "turbo run db:generate",
    "db:push": "turbo run db:push",
    "db:migrate": "turbo run db:migrate",
    "db:studio": "yarn workspace @zakazi-termin/prisma db:studio",
    "clean": "turbo run clean && rm -rf node_modules",
    "test:e2e": "yarn workspace @zakazi-termin/web test:e2e",
    "test:e2e:ui": "yarn workspace @zakazi-termin/web test:e2e:ui",
    "test:db:start": "docker compose -f docker-compose.test.yml up -d",
    "test:db:stop": "docker compose -f docker-compose.test.yml down"
  },
  "devDependencies": {
    "turbo": "^2.3.0",
    "typescript": "^5.7.2"
  }
}
